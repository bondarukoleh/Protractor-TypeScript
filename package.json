{
  "name": "protractor-typescript",
  "version": "1.0.0",
  "description": "Test framework with protractor.",
  "main": "index.js",
  "scripts": {
    "start:driver": "./node_modules/.bin/webdriver-manager start",
    "lint": "tslint -e './node_modules/' -p ./tsconfig.json",
    "pretest": "npm run lint",
    "report": "rm -rf ./allure-report && allure generate ./allure-results -o ./allure-report && allure open ./allure-report",
    "test": "./node_modules/.bin/protractor ./protractor.conf.js",
    "test:local": "./node_modules/.bin/protractor ./protractor.conf.js --local",
    "tsc": "tsc",
    "tscw": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bondarukoleh/protractor-typescript.git"
  },
  "author": "Oleh Bondaruk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bondarukoleh/protractor-typescript/issues"
  },
  "homepage": "https://github.com/bondarukoleh/protractor-typescript#readme",
  "dependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "2.2.41",
    "@types/node": "8.0.24",
    "allure-commandline": "^2.9.0",
    "chai": "4.1.1",
    "jira-client": "^6.7.0",
    "log4js": "^2.6.1",
    "minimist": "^1.2.0",
    "mocha": "^6.0.2",
    "mocha-allure-reporter": "^1.4.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.17",
    "precommit-hook": "^3.0.0",
    "protractor": "^5.4.2",
    "request-promise": "^4.2.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "3.1.6"
  },
  "pre-commit": [
    "lint",
    "tsc"
  ]
}
